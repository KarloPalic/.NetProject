<Window x:Class="WindowsPresentationFoundationApplication.TeamStats"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowsPresentationFoundationApplication.Resources"
        mc:Ignorable="d"
        Title="TeamStats" Height="198" Width="258">
    <Grid >
        <Label Content="{x:Static local:Resource1.TeamNameText}" HorizontalAlignment="Left" Margin="49,27,0,0" VerticalAlignment="Top"/>
        <Label Content="{x:Static local:Resource1.FifaCodeText}" HorizontalAlignment="Left" Margin="49,71,0,0" VerticalAlignment="Top"/>
        <Label Content="{x:Static local:Resource1.GroupLetterText}" HorizontalAlignment="Left" Margin="49,116,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbTeamName" Content="Label" HorizontalAlignment="Left" Margin="130,27,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbFifaCode" Content="Label" HorizontalAlignment="Left" Margin="114,71,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbGroupLetter" Content="Label" HorizontalAlignment="Left" Margin="134,116,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
